    "type": "contao-module",
    "homepage": "https://github.com/Teamsisu/contao-mm-frontendInput",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Martin Treml",
            "email": "martin.treml@teamsisu.at",
            "homepage": "http://www.teamsisu.at",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Teamsisu/contao-mm-frontendInput/issues",
